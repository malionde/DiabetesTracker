@using MVCExample.Models
@model IEnumerable<AddBloodSugar>

@{
    ViewBag.Title = "Details";

}
@Styles.Render("~/Content/toastr")
@Styles.Render("~/Css")
<section class="content">
    <div class="row">
        <div class="col-xs-12">
            <div class="box">
                <div class="box-header">
                    <h3 class="box-title">Blood Sugar Levels</h3>
                </div>
                <!-- /.box-header -->
                <div class="box-body">
                    <div id="example2_wrapper" class="dataTables_wrapper form-inline dt-bootstrap">
                        <div class="row"><div class="col-sm-6"></div><div class="col-sm-6"></div></div><div class="row">
                            <div class="col-sm-12">
                                <table id="example2" class="table table-bordered table-hover dataTable" role="grid" aria-describedby="example2_info">
                                    <thead>
                                        <tr role="row">
                                            <th class="sorting_asc" tabindex="0" aria-controls="example2" rowspan="1" colspan="1" aria-sort="ascending" aria-label="Rendering engine: activate to sort column descending">Blood Sugar Level</th>
                                            <th class="sorting" tabindex="0" aria-controls="example2" rowspan="1" colspan="1" aria-label="Browser: activate to sort column ascending">Date</th>
                                        </tr>
                                    </thead>
                                    <tbody>

                                        @foreach (var i in Model)
                                        {
                                            <tr role="row" class="odd">
                                                <td class="sorting_1">
                                                    <p>
                                                        @i.BloodSugarValue
                                                    </p>
                                                </td>
                                                <td class="sorting_1">
                                                    <p> @i.BloodSugarEntranceDate</p>
                                                </td>
                                            </tr>

                                        }

                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                    <!-- /.box-body -->
                </div>
            </div>
        </div>
    </div>
</section>